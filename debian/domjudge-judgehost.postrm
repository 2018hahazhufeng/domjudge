#! /bin/sh -e

umount /var/lib/domjudge/javachroot/proc || true
umount /var/lib/domjudge/javachroot.new/proc || true

if [ "$1" = "purge" ]; then
	# do not purge entire /v/l/domjudge, because domjudge-domserver
	# may still remain installed
	rm -rf /var/log/domjudge/judge.*.log /var/lib/domjudge/judgings \
		/var/lib/domjudge/javachroot /var/lib/domjudge/javachroot.new
fi

#DEBHELPER#

exit 0
