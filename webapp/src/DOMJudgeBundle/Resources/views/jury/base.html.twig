{% extends "::base_bootstrap.html.twig" %}
{% import _self as self %}

{% block title %}{{ title|default('DOMjudge') }}{% endblock %}

{% block extrahead %}
<link rel="stylesheet" href="{{asset('style_domjudge.css')}}">
<link rel="stylesheet" href="{{asset('style_jury.css')}}">

{% if jscolor is defined %}
<script src="{{asset('js/jscolor.js')}}"></script>
{% endif %}

{% if jqtokeninput is defined %}
  <link rel="stylesheet" href="{{asset('token-input.css')}}">
  <script src="{{asset('js/jquery.tokeninput.min.js')}}"></script>
{% endif %}

{# <script src="{{asset('js/jury.js')}}"></script> #}

{{ extrahead|default('') }}
{% endblock %}

{% block body %}
{% include '::header_bootstrap.html.twig' %}
<div class="container-fluid">
  <div class="row">
    <div class="col-12">
      {% block content %}{{ body|raw }}{% endblock %}
    </div>
  </div>
</div>
{% endblock %}
